!setup.pl -d=2

 &filelist
        base_filename='testRZstretchcentral'  ! Base file name for dat/vtu output
        saveprim=T
        autoconvert=T
	convert_type='vtuBCCmpi'
        nwauxio=5
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave_log=0.05          ! For the log
        dtsave_dat=0.5d0         ! For dat/vtu output
 /

 &stoplist
        dtmin=1.0d-9
        it_max=10
 /

 &methodlist
        time_stepper=  'fivestep'
        flux_scheme=20*'hllc'
        limiter=20*'ppm'
        typecurl='central'
 /
        typecurl='Gaussbased'

 &boundlist
        typeboundary_min1 = 'symm','asymm','symm','symm','asymm','symm'
        typeboundary_max1 = 6*'cont'
        typeboundary_min2 = 6*'periodic'
        typeboundary_max2 = 6*'periodic'
 /

 &meshlist
        stretch_dim(1)='uni'
        stretch_dim(2)='symm'
        nstretchedblocks_baselevel(1)=2
        nstretchedblocks_baselevel(2)=2
        qstretch_baselevel(1)=1.03
        qstretch_baselevel(2)=1.3
        refine_max_level=6
        w_refine_weight(1)=0.5d0
        w_refine_weight(6)=0.5d0
        domain_nx1=64
        domain_nx2=32
        block_nx1=16
        block_nx2=16
        xprobmin1=0.0d0
        xprobmax1=10.0d0
        xprobmin2=0.0d0
        xprobmax2=10.0d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.8d0
        slowsteps=10
 /

 &mhd_list
        mhd_energy = .true.
        typedivbfix = 'linde'
 /

 &usr_list
        Rjet= 1.5d0
        B0=1.0d0
        Bc=0.01d0
        rhojet=1.0d0
        rhocloud=10.0d0
        pjet=2.0d0
 /
