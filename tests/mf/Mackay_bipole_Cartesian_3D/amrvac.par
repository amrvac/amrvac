! setup.pl -d=3
 &filelist
	base_filename='bipole_mfb'
        autoconvert=.false.
        saveprim=.true.
        nwauxio=7
	convert_type='vtuBCCmpi'
 /

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave_dat=1.d0
        ditsave_log=2
        ditsave_custom=2
 /
        dtsave_custom=0.01d0
        dtsave_log=0.01d0

 &stoplist
        time_max=20.d0
        it_max=10
 /

 &methodlist
        time_stepper='twostep'
        flux_scheme=20*'hll'
        limiter=20*'cada3'
 /

 &boundlist
       typeboundary_min1=6*'special'
       typeboundary_max1=6*'special'
       typeboundary_min2=6*'special'
       typeboundary_max2=6*'special'
       typeboundary_min3=6*'special'
       typeboundary_max3=6*'special'
 / 

 &meshlist
        refine_criterion=3
        refine_max_level=3
        w_refine_weight(4)=0.3
        w_refine_weight(5)=0.3
        w_refine_weight(6)=0.4
        block_nx1=16
        block_nx2=16
        block_nx3=16
        domain_nx1=96
        domain_nx2=64
        domain_nx3=64
        xprobmin1=-4d0
        xprobmax1=4d0
        xprobmin2=-3d0
        xprobmax2=3d0
        xprobmin3=0d0
        xprobmax3=6d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.8d0
 /

 &mf_list
        Busr=50.d0
        typedivbfix='ct'
        mf_decay_scale(1)=0.d0
        mf_decay_scale(2)=0.d0
        mf_decay_scale(3)=0.d0
        mf_decay_scale(4)=0.d0
        mf_decay_scale(5)=1.d0
        mf_decay_scale(6)=0.d0
 /
        clean_initial_divb=.true.
