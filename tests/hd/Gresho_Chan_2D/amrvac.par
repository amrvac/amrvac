!setup.pl -d=2

 &filelist
        saveprim=T
        autoconvert=T
	convert_type='vtuBCCmpi'
        base_filename='Gresho'  ! Base file name for dat/vtu output
        typefilelog='special'
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave_log=0.05          ! For the log
        dtsave_dat=0.05          ! For dat/vtu output
 /

 &stoplist
        dtmin=1.0d-6
        time_max=12.0d0
 /

 &methodlist
        time_stepper=  'threestep'
        flux_scheme=20*'hll'
        limiter=20*'woodward'

 /

 &boundlist
        typeboundary_min1 = 4*'periodic'
        typeboundary_max1 = 4*'periodic'
        typeboundary_min2 = 4*'periodic'
        typeboundary_max2 = 4*'periodic'
 /

 &meshlist
        refine_max_level=5
        domain_nx1=100
        domain_nx2=100
        block_nx1=10
        block_nx2=10
        xprobmin1=-0.5d0
        xprobmax1=0.5d0
        xprobmin2=-0.5d0
        xprobmax2=0.5d0
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.9d0
        slowsteps=10
 /

 &usr_list
        v01=1.0d0
        v02=1.0d0
        Mach=0.346410d0
 /
